pileline {
    agent any

    stages{
        stage('begin'){
            steps {
                    echo 'hello pipeline begin'
                    sleep 5
                   }
            }

         stage('running'){
            steps {
                    echo 'hello pipeline running'
                    sleep 5
                    }
            }

         stage('finish'){
            steps {
                    echo 'hello pipeline finish'
                    sleep 5
                   }
            }

         }
}